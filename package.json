{
    "name": "js_template",
    "version": "1.0.0",
    "description": "template for a small js project",
    "main": "index.js",
    "scripts": {
        "start": "node src/index.js",
        "test": "jest --collectCoverage --coverageDirectory='src/coverage'",
        "create-doc": "jsdoc --readme README.md -c .jsdoc.conf.json -d src/docs",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "keywords": [
        "template"
    ],
    "author": "Abdolkarim Saeedi",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "jest": "^29.0.1",
        "jsdoc": "^3.6.11"
    },
    "devDependencies": {
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "prettier": "^2.7.1"
    }
}
